

<div class="fui-form-link">
    <div class="fui-form-ele-container">
        <div class="right" data-form-link-a>
            {{#if color}}
            <a class="fui-form-string-a" style="color:{{color}};">{{text}}</a>
            {{else}}
            <a class="fui-form-string-a">{{text}}</a>
            {{/if}}
        </div>
    </div>
    <div class="fui-form-ele-error-msg"></div>

</div>