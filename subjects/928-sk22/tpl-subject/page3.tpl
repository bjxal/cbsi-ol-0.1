<div class="page3">
    <a class="pro pro1" data-tap="p3-pro@1"></a>
    <a class="pro pro2" data-tap="p3-pro@2"></a>
    <a class="pro pro3" data-tap="p3-pro@3"></a>
    <a class="pro pro4" data-tap="p3-pro@4"></a>
    <div class="popup popup1">
        <img data-tap="p3-popup@1" src="{{popup1}}"/>
        <a class="close" data-tap="p3-close@1"></a>
    </div>
    <div class="popup popup2">
        <img data-tap="p3-popup@2" src="{{popup2}}"/>
        <a class="close" data-tap="p3-close@2"></a>

    </div>
    <div class="popup popup3">
        <img data-tap="p3-popup@3" src="{{popup3}}"/>
        <a class="close" data-tap="p3-close@3"></a>

    </div>
    <div class="popup popup4">
        <img data-tap="p3-popup@4" src="{{popup4}}"/>
        <a class="close" data-tap="p3-close@4"></a>
    </div>
    <img data-tap="p3-tip" class="popup tip show" src="{{tip}}"/>
    <div class="popup success">
        <div data-tap="success" class="s s1"></div>
        <div data-tap="success" class="s s2"></div>
        <div data-tap="success" class="s s3"></div>
        <div data-tap="success" class="s s4"></div>
    </div>
</div>